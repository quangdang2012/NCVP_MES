    sap_item.sap_mrppp_ppc_planning_calendar, \n    sap_item.sap_fhori_scheduling_margin_keys, \n    sap_item.sap_eisbe_safety_stock, \n    sap_item.sap_lgrad_service_level, \n    sap_item.sap_eislo_min_safety_stock, \n    sap_item.sap_rwpro_coverage_profile, \n    sap_item.sap_shflg_safety_time_indicator, \n    sap_item.sap_shzet_safety_time, \n    sap_item.sap_shpro_safety_time_period, \n    sap_item.sap_strgr_planning_strategy_group, \n    sap_item.sap_vrmod_consumption_mode, \n    sap_item.sap_vint1_consumption_period, \n    sap_item.sap_vint2_consumption_period, \n    sap_item.sap_miskz_mixed_mrp_indicator, \n    sap_item.sap_mtvfp_checking_group, \n    sap_item.sap_altsl_altsl, \n    sap_item.sap_kausf_component_scrap, \n    sap_item.sap_kzaus_discontinuation_indicator, \n    sap_item.sap_ausdt_interrupt_date, \n    sap_item.sap_nfmat_follow_up_material, \n    sap_item.registration_user_cd, \n    sap_item.registration_date_time, \n    sap_item.factory_cd\n   FROM sap_item\n   left join sap_item itm2 on sap_item.sap_matnr_item_cd = itm2.sap_matnr_item_cd\n   and sap_item.registration_date_time < itm2.registration_date_time  and sap_item.factory_cd = itm2.factory_cd                            \n  Where  itm2.registration_date_time ISNULL;
